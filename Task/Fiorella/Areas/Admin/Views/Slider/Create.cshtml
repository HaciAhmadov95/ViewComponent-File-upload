@model SliderCreateVM
@{
    ViewData["Title"] = "Create";
}


<div class="container mt-5">
    <form class="forms-sample" method="post" asp-action="Create">
        <div class="form-group">
            <label>Image</label>
            <input asp-for="Image" class="form-control">
            <span class="text-danger" asp-validation-for="Image"></span>
        </div>



        <button type="submit" class="btn btn-primary text-white me-2">Submit</button>
        <a asp-action="Index" class="btn btn-primary text-white">Back</a>
    </form>
</div>



